{
    "manifest_version": 3,
    "name": "ChatGPT Query(OpenChatGPTAnywhere)",
    "version": "1.0",
    "action": {
      
    },
    "permissions": ["activeTab","webRequest"],
    "background": { "service_worker": "background.js"},
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
